#    --with-mutex=x86/gcc-assembly
#    --enable-posixmutexes
cd build_unix
../dist/configure \
    --prefix=/usr \
    --libdir=/usr/lib64 \
    --enable-o_direct \
    --with-cryptography=no \
    --enable-compat185 \
    --disable-sql \
    --enable-shared \
    --enable-static \
    --enable-cxx \
    --enable-stl \
    --enable-tcl \
    --with-tcl=/usr/lib64 \
    --enable-test
